{
  "name": "be",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server": "nodemon ./bin/www",
    "client": "concurrently \"cd client && npm install\" \"npm run build --prefix client\"",
    "start": "concurrently \"npm run server\" \"npm start --prefix client\"",
    "heroku-postbuild": "npm install --no-optional --prefix client && npm run build --prefix client",
    "heroku-prebuild": "npm install --no-optional"
  },
  "dependencies": {
    "concurrently": "^6.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongodb": "^3.6.9",
    "mongoose": "^5.13.2",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.12"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
